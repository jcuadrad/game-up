<div class="create-container">
<h1>
    CREATE A GAME
</h1>
    <form method="POST" action="/new">
        <input type="text" name="name" placeholder="Name">
        
        
        <select id="sport" name="sport" placeholder="Sport">
            <option value="" disabled selected hidden>Choose a sport</option>
            <option value="Volleyball">Volleyball</option>
            <option value="Football">Football</option>
            <option value="Basketball">Basketball</option>
            <option value="Paddel">Paddel</option>
        </select>
        
       <label>Start & End Time</label>
        <input id="startTime" name="startTime" type="datetime-local" placeholder="When does it start?">
        
        
      
        <input id="endTime" name="endTime" type="datetime-local" placeholder="When does it end?">
        
        
      
        <input type="number" name="playersNeeded" placeholder="Players needed?">
        
        <input id="searchTextField" type="text" size="50" placeholder="Where is your game?">
        <div id="search-map"></div>
        
        <input type="hidden" id="cityLat" name="cityLat" />
        <input type="hidden" id="cityLng" name="cityLng" />
        <!-- <label>Address</label>
        <input type="text" name="latitude" placeholder="Latitude">
        <input type="text" name="longitude" placeholder="Longitude"> -->

        <input type="submit" name="">
    </form>
</div>
    <script 
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDe1EOMCU_S7p8fuPO5HQxCp6BDgDde-H0&libraries=places">
  </script>
